
#include <iostream>

#include <cmath>

#include <vector>

#include <algorithm>

#include <limits>
//trintento
using namespace std;
int main() {
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        int m;
        cin >> m;
        vector<vector<int>> l;
        while (true) {
            vector<int> v(2);
            cin >> v[0] >> v[1];
            if (v[0] == 0 && v[1] == 0) {
                break;
            }
            l.push_back(v); //ALGORITMO VORAZ TECHNIQUE
        }
        sort(l.begin(), l.end());
        int r = 0;
        for (int i = 0; i < l.size(); i++) {
            if (l[i][0] <= m && l[i][1] >= m) {
                r += 1;
                break;
            }
        }
        if (r == 0) {
            cout << 0 << endl;
        } else {
            cout << r << endl;
            for (int i = 0; i < l.size(); i++) {
                if (l[i][0] <= m && l[i][1] >= m) {
                    cout << l[i][0] << " " << l[i][1] << endl;
                    break;
                }
            }
        }
        cout << endl;
    }
    return 0;
}
